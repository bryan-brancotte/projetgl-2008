<?xml version="1.0" encoding="UTF-8"?><xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">	<xs:element name="Kind">		<xs:simpleType>			<xs:restriction base="xs:string">				<xs:enumeration value="Problem"/>				<xs:enumeration value="Solution"/>			</xs:restriction>		</xs:simpleType>	</xs:element>	<xs:complexType name="RouteType">		<xs:sequence>			<xs:element name="MSID" type="xs:int"/>			<xs:element name="IDR" type="xs:string"/>			<xs:element ref="Kind"/>			<xs:element name="WarningMsg" type="xs:string"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="RouteListType">		<xs:sequence>			<xs:element name="Route" type="RouteType" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="StationType">		<xs:sequence>			<xs:element name="MSID" type="xs:int"/>			<xs:element name="ID" type="xs:int"/>			<xs:element ref="Kind"/>			<xs:element name="WarningMsg" type="xs:string"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="StationRouteType">		<xs:sequence>			<xs:element name="MSID" type="xs:int"/>			<xs:element name="IDS" type="xs:int"/>			<xs:element name="IDR" type="xs:string"/>			<xs:element ref="Kind"/>			<xs:element name="WarningMsg" type="xs:string"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="StationRouteListType">		<xs:sequence>			<xs:element name="StationRoute" type="StationRouteType" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<xs:complexType name="StationsListType">		<xs:sequence>			<xs:element name="Station" type="StationType" maxOccurs="unbounded"/>		</xs:sequence>	</xs:complexType>	<xs:element name="TravelAlert">		<xs:complexType>			<xs:sequence>				<xs:element name="VersionNumber" type="xs:int" nillable="false"/>				<xs:element name="StationsList" type="StationsListType"/>				<xs:element name="StationRouteList" type="StationRouteListType"/>				<xs:element name="RouteList" type="RouteListType"/>			</xs:sequence>		</xs:complexType>	</xs:element>	<xs:element name="VersionNumber" type="xs:byte"/>	<xs:element name="WarningMsg" type="xs:string"/></xs:schema>